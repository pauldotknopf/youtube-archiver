@model ChannelModel

<div class="container">
    @foreach (var video in Model.Videos)
    {
        <div class="post">
            <h2 class="title">
                <a href="@Url.Content($"~/video/{video.Id}")">@video.Title</a>
            </h2>
            <p class="submitted">
                uploaded on @(video.UploadedOn.HasValue ? @video.UploadedOn.Value.ToString("MMM dd, yyyy") : "(unknown)")
            </p>
        </div>
    }
</div>